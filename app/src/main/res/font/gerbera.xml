<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:fontStyle="normal"
        android:fontWeight="200"
        android:font="@font/gerbera_light"
        app:fontStyle="normal"
        app:fontWeight="200"
        app:font="@font/gerbera_light"
        />

    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/gerbera_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/gerbera_regular"
        />

    <font
        android:fontStyle="normal"
        android:fontWeight="500"
        android:font="@font/gerbera_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/gerbera_medium"
        />

    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/gerbera_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/gerbera_bold"
        />

    <font
        android:fontStyle="normal"
        android:fontWeight="900"
        android:font="@font/gerbera_black"
        app:fontStyle="normal"
        app:fontWeight="900"
        app:font="@font/gerbera_black"
        />

</font-family>